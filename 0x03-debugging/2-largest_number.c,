#include "main.h"
/**
* largest_number - returns the largest of 3 numbers
* @a: first integer
* @b: second integer
* @c: third integer
* Return: largest number
*/
int largest_number(int a, int b, int c)

{
	int biggest;

	if (a >= b && a >= c)
	{
		biggest = a;
	}
	else if (b >= a && b >= c)
	{
		biggest = b;
	}
	else
	{
		biggest = c;
	}
	return (biggest);
}
